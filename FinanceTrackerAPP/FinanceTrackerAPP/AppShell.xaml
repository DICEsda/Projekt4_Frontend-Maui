<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="FinanceTrackerAPP.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:FinanceTrackerAPP"
    xmlns:pages="clr-namespace:FinanceTrackerAPP.Views"
    Shell.FlyoutBehavior="Flyout"
    Title="FinanceTrackerAPP">

    <ShellContent Title="Register" ContentTemplate="{DataTemplate pages:Register}" Route="Register" />
    <ShellContent Title="Home" ContentTemplate="{DataTemplate pages:MainPage}" Route="MainPage" />
    <ShellContent Title="Add_Edit" ContentTemplate="{DataTemplate pages:Add_Edit}" Route="Add_Edit" />

</Shell>
