<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="FinanceTrackerAPP.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:FinanceTrackerAPP"
    Shell.FlyoutBehavior="Flyout"
    Title="FinanceTrackerAPP">

    <!-- First ShellContent for MainPage -->
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <!-- Second ShellContent for NewPage1 -->
    <ShellContent
        Title="Newpage1"
        ContentTemplate="{DataTemplate local:NewPage1}"
        Route="NewPage1" />

</Shell>